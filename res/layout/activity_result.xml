<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wood_background"
    tools:context="com.majdzain.iq.ResultActivity" >
    
    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:text="@string/result"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/bnfsg" />

    <TextView
        android:id="@+id/qtext2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView1"
        android:layout_alignBottom="@+id/textView1"
        android:layout_toLeftOf="@+id/textView1"
        android:text="@string/shhh"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/red" />

    <TextView
        android:id="@+id/qtext3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView1"
        android:layout_alignBottom="@+id/textView1"
        android:layout_toRightOf="@+id/textView1"
        android:text="@string/shhh"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/red" />

    <TextView
        android:id="@+id/qin1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        android:background="@color/rmade"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/green" />

    <TextView
        android:id="@+id/qin2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/qin1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp"
        android:text="@string/smartis"
        android:textColor="@color/black"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/smartr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/qin2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#FF9900"
        android:textSize="60sp" />

    <TextView
        android:id="@+id/smart2r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ager"
        android:layout_below="@+id/ager"
        android:text="TextView"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/qin4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ager"
        android:layout_alignBottom="@+id/ager"
        android:layout_toRightOf="@+id/ager"
        android:text="@string/sh"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ager"
        android:layout_toLeftOf="@+id/ager"
        android:text="@string/sh"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/qin6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/smart2r"
        android:layout_alignBottom="@+id/smart2r"
        android:layout_alignRight="@+id/textView8"
        android:text="@string/sh"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/qin7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ager"
        android:layout_toRightOf="@+id/ager"
        android:text="@string/sh"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="7dp"
        android:layout_alignLeft="@+id/smartr"
        android:layout_alignRight="@+id/smartr"
        android:layout_below="@+id/smartr"
        android:background="#FF9900"
        android:text="-------"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#FF9900" />

    <TextView
        android:id="@+id/smart1r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/white"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/ager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/smart1r"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        android:text="TextView"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:text="100"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/blue"
        android:textSize="35sp" />

    <Button
        android:id="@+id/restart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/qin6"
        android:text="إعادة بدء"
        android:textColor="@color/green" />

    <Button
        android:id="@+id/stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/restart"
        android:layout_alignBottom="@+id/restart"
        android:layout_alignParentLeft="true"
        android:text="خروج"
        android:textColor="@color/red" />

</RelativeLayout>
